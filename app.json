{
  "expo": {
    "name": "safekid",
    "slug": "safekid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/shared/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robertw8.safekid"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.robertw8.safekid"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/shared/assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "89d38f90-72e5-4911-89fd-5ea912bb71e3"
      }
    }
  }
}
