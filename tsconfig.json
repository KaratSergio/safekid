{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"isolatedModules": true,
		"incremental": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
